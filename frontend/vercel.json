{
    "version": 2,
    "buildCommand": "npm run build",
    "outputDirectory": "frontend/dist",
    "installCommand": "npm install",
    "routes": [
        {
            "src": "/api/auth/(.*)",
            "dest": "/api/auth.js"
        },
        {
            "src": "/api/events/(.*)",
            "dest": "/api/events.js"
        },
        {
            "src": "/api/registrations/(.*)",
            "dest": "/api/registrations.js"
        },
        {
            "handle": "filesystem"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}