{
    "version": 2,
    "builds": [
        {
            "src": "frontend/package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "frontend/dist"
            }
        }
    ],
    "routes": [
        {
            "src": "/api/auth/(.*)",
            "dest": "/api/auth.js"
        },
        {
            "src": "/api/events/(.*)",
            "dest": "/api/events.js"
        },
        {
            "src": "/api/registrations/(.*)",
            "dest": "/api/registrations.js"
        },
        {
            "handle": "filesystem"
        },
        {
            "src": "/(.*)",
            "dest": "/frontend/index.html"
        }
    ]
}